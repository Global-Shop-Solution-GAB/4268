					F.Intrinsic.String.Build("{0}\GAB_4228_WC_Export.xlsx",V.Caller.LocalGssTempDir,V.Local.sFileExport)
					F.Intrinsic.File.IsFileLocked(V.Local.sFileExport,V.Local.bFileLocked)
